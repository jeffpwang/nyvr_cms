{
	"version": <%= @version %>, 
	"data": [
		<% @contents.each do |content| %>
			{
				"name": "<%= content.title %>", 
				"thumbnail": "<%= request.base_url + content.thumbnail.url %>",
				"content": "<%= request.base_url + content.file.url %>"
			},
		<% end %>
	], 
	"url": <%= request.original_url %>
}
